@page "/analysis"

<MudPaper Class="pa-16 ma-2" Elevation="3">
    <MudText Typo="Typo.h4">Analyse (Beispiel LIDL)</MudText>

    @if (_storePriceChanges is not null)
    {
        @foreach (var storePriceChange in _storePriceChanges)
        {
            <MudText>@storePriceChange.Category @storePriceChange.Date : @storePriceChange.PriceUnit / @storePriceChange.PriceBulk</MudText>
        }
    }

    

</MudPaper>


